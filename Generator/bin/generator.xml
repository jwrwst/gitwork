     <entry key="jdbc.url">jdbc:mysql://139.129.110.116:3306/bobocenter?useUnicode=true&amp;characterEncoding=UTF-8</entry>
     <entry key="jdbc.driver">com.mysql.jdbc.Driver</entry>
      -->
     
</properties>
